<Application x:Class="WpfEfSampleApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:System="clr-namespace:System;assembly=Mscorlib"
             StartupUri="Page1.xaml" NavigationFailed="Application_NavigationFailed">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="BlueSkin.xaml"/>
                <ResourceDictionary Source="EfDictionary.xaml"/>
                <ResourceDictionary Source="Button.xaml"/>
                <ResourceDictionary Source="ListBox.xaml"/>
                <ResourceDictionary Source="ShinyBlue.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
